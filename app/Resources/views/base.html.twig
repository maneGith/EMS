 // Function to convert 
        function isReel(x) {
            var chars = decodeURI(x).trim().replace(' ', '').split('');
            var result=true;
            var point=0;
            
            for (i = 0; i < chars.length; ++i) {
                
                if(chars[i]!='.' && chars[i]!='0'){
                    if(!parseInt(chars[i])){
                        result=false;
                        break;
                    }
                }else if(chars[i]=='.'){
                point=point+1;
                }
            }
            
            if(point>1){
                result=false;
            }
            return result;
        }
        
        
